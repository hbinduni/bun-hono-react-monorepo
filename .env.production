# Production Environment Variables
# Copy this file to .env.production.local and update with your actual values
# DO NOT commit .env.production.local to version control

# Application
NODE_ENV=production

# PostgreSQL Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=app_db
POSTGRES_PORT=5432

# Server
SERVER_PORT=3000

# Client
CLIENT_PORT=80
VITE_API_URL=http://localhost:3000

# Docker
COMPOSE_PROJECT_NAME=monorepo
